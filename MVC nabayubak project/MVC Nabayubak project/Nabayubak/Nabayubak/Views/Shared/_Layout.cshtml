                    </button>
                    @*@Html.ActionLink("Nabayubak", "Index", "Naba", null, new { @class = "navbar-brand" })*@
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        
                        <li>@Html.ActionLink("Contestant", "Index", "Naba")</li>
